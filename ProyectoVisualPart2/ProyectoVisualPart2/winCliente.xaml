<Window x:Class="winCliente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoVisualPart2"
        mc:Ignorable="d"
        Title="winCliente" Height="300" Width="603.704">
    <Grid>
        <DataGrid x:Name="dtgClientes" Margin="0,22,0,137" ItemsSource="{Binding Path=cliente}" AutoGenerateColumns="False" BorderBrush="#FF377EC3">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding precio}" Header="Nombre"/>
                <DataGridTextColumn Binding="{Binding formula}" Header="Apellido"/>
                <DataGridTextColumn Binding="{Binding indicaciones}" Header="Cedula"/>
                <DataGridTextColumn Binding="{Binding costo}" Header="Edad"/>
                <DataGridTextColumn Binding="{Binding precio}" Header="Email"/>
                <DataGridTextColumn Binding="{Binding indicaciones}" Header="Telefono"/>
                <DataGridTextColumn Binding="{Binding indicaciones}" Header="Genero"/>
                <DataGridTextColumn Binding="{Binding indicaciones}" Header="Tipo"/>
            </DataGrid.Columns>


        </DataGrid>

    </Grid>
</Window>
