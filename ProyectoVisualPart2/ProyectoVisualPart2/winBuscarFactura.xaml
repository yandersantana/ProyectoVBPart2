<Window x:Name="winBuscarFac" x:Class="winBuscarFactura"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoVisualPart2"
        mc:Ignorable="d"
        Title="winBuscarFactura" Height="300" Width="300">
    <Grid Margin="0,0,2,2">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"/>
            <ColumnDefinition Width="267"/>
            <ColumnDefinition Width="18*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Content="Ingrese el código de la factura a buscar" Grid.Column="1" HorizontalAlignment="Left" Margin="25,57,0,0" VerticalAlignment="Top" Width="221" Height="36" Background="#FFF0DFD9"/>
        <TextBox x:Name="codFac" Grid.Column="1" HorizontalAlignment="Left" Height="35" Margin="21,133,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="236"/>
        <Button x:Name="salir" Content="Salir" Grid.Column="1" HorizontalAlignment="Left" Margin="91,205,0,0" VerticalAlignment="Top" Width="87" Height="34" Background="#FFE08326" BorderBrush="White" Foreground="#FFFFFDFD"/>

    </Grid>
</Window>
