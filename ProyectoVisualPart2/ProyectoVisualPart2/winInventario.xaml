<Window x:Name="winInventar" x:Class="winInventario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoVisualPart2"
        mc:Ignorable="d"
        Title="winInventario" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textTotalVentas" HorizontalAlignment="Left" Height="23" Margin="78,137,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsEnabled="False"/>
        <Label x:Name="label" Content="Total Ventas" HorizontalAlignment="Left" Margin="92,88,0,0" VerticalAlignment="Top" Width="91"/>
        <Button x:Name="salir" Content="Salir" HorizontalAlignment="Left" Margin="70,206,0,0" VerticalAlignment="Top" Width="128" Height="48" Background="#FFF1CAAE" BorderBrush="White"/>

    </Grid>
</Window>
